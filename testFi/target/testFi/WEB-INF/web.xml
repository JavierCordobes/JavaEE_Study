<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<display-name>HelloServlet</display-name>
  	<description></description>
  	<servlet-class>testFi.HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletFormulario</servlet-name>
  	<display-name>ServletFormulario</display-name>
  	<description></description>
  	<servlet-class>serv.ServletFormulario</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FormServ</servlet-name>
  	<display-name>FormServ</display-name>
  	<description></description>
  	<servlet-class>packa.FormServ</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/helloServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletFormulario</servlet-name>
  	<url-pattern>/servletFormulario</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FormServ</servlet-name>
  	<url-pattern>/formServ</url-pattern>
  </servlet-mapping>
</web-app>
